<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import com.dbboys.customnode.CustomLabelTextField?>
<?import com.dbboys.customnode.CustomUserTextField?>
<?import com.dbboys.customnode.CustomInfoStackPane?>
<?import com.dbboys.customnode.CustomResultsetTableView?>
<VBox fx:id="resultSetVBox" style="-fx-border-width: 0;-fx-border-color: #008000;-fx-background-insets: 0;-fx-background-color: #eee;" fx:controller="com.dbboys.ctrl.ResultSetTabController" xmlns="http://javafx.com/javafx/8.0.31" xmlns:fx="http://javafx.com/fxml/1">
    <HBox alignment="CENTER_LEFT" style="-fx-background-color: #f2f2f2;-fx-border-width:0.2 0 0 0;-fx-border-color: white">
        <Button fx:id="lastSqlCopyButton" styleClass="little-custom-button" focusTraversable="false" />
        <CustomLabelTextField fx:id="lastSqlTextField" text="" focusTraversable="false" />
        <Region HBox.hgrow="ALWAYS"></Region>
        <Button fx:id="lastSqlRefreshButton" styleClass="little-custom-button" focusTraversable="false" />
    </HBox>
    <StackPane>
        <VBox style="-fx-border-color: none;">
            <HBox style="-fx-background-color: white;">
                <CustomResultsetTableView fx:id="resultSetTableView">
                </CustomResultsetTableView>
            </HBox>
            <HBox fx:id="resultSetButtonHBox" minHeight="20" alignment="CENTER_LEFT" style="-fx-border-color: #aaa;-fx-border-width: 0.2 0 0 0">
                <Label fx:id="resultSetLabelLeftBracket" />
                    <StackPane>
                        <Label visible="false" fx:id="resultSetEditableEnabledLabel" />
                        <Label fx:id="resultSetEditableDisabledLabel" />
                    </StackPane>
                <Label fx:id="resultSetLabelRightBracket" />
                <Label fx:id="resultSetLabelPerTimePrefix" />
                <CustomUserTextField fx:id="resultSetPerTimeTextField" text="200" style="-fx-pref-width: 50;-fx-padding: 0;-fx-font-size: 11" >
                </CustomUserTextField>

                <Label fx:id="resultSetLabelBetweenNext" />
                <Button fx:id="resultSetNextPageButton" styleClass="little-custom-button" focusTraversable="false" />
                <Label fx:id="resultSetLabelBetweenAll" />
                <Button fx:id="resultSetAllRowsButton" styleClass="little-custom-button" focusTraversable="false" />

                <Label fx:id="resultSetLabelFetchedPrefix" />
                <Label fx:id="resultSetFetchedRowsLabel">?</Label>
                <Label fx:id="resultSetLabelFetchedSuffix" />
                <Button fx:id="resultSetCountButton" styleClass="little-custom-button" focusTraversable="false" />
                <Label fx:id="resultSetLabelTotalPrefix" />
                <Label fx:id="resultSetTotalRowsLabel">?</Label>
                <Label fx:id="resultSetLabelTotalSuffix" />
                <Label fx:id="sqlUsedTimeLabel">?</Label>
                <Label fx:id="resultSetLabelTimeSuffix" />
                <Button fx:id="resultSetExportButton" styleClass="little-custom-button" focusTraversable="false" />
                <Label fx:id="resultSetLabelEnd" />
            </HBox>
        </VBox>
    </StackPane>
</VBox>

